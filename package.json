{
  "private": true,
  "name": "votingdeadlines",
  "version": "0.0.1",
  "description": "WIP: Parse, validate, and republish voting deadline info.",
  "author": "zionillinois@users.noreply.github.com",
  "license": "ISC",
  "scripts": {
    "data:trim:vo": "yarn tsn bin/trimVoteOrgHtml.ts dotenv_config_path=config.sh",
    "data:extract:vo": "yarn tsn bin/extractVoteOrgData.ts dotenv_config_path=config.sh",
    "data:clean:vo": "yarn tsn bin/cleanVoteOrgData.bin.ts dotenv_config_path=config.sh",
    "format": "yarn prettier --write",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:prettier": "yarn prettier --check --debug-check",
    "prettier": "prettier bin src",
    "test": "yarn lint && yarn test:jest",
    "test:jest": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "tsn": "ts-node -r dotenv/config"
  },
  "engines": {
    "node": ">= 12.18"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.13",
    "@types/jsdom": "^16.2.4",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.1",
    "jest": "^26.4.2",
    "jsdom": "^16.4.0",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
