{
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "svelteAllowShorthand": false,
  "svelteBracketNewLine": true,
  "svelteIndentScriptAndStyle": false,
  "svelteSortOrder" : "styles-scripts-markup",
  "svelteStrictMode": true,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
