{
  "private": true,
  "name": "votingdeadlines-data",
  "version": "0.0.1",
  "description": "WIP: Parse, validate, and republish voting deadline info.",
  "author": "zionillinois@users.noreply.github.com",
  "license": "ISC",
  "scripts": {
    "console": "yarn tsn src/console.ts dotenv_config_path=config.sh",
    "data:trim:va": "yarn tsn bin/voteamerica.com/trimVoteAmericaHtml.bin.ts dotenv_config_path=config.sh",
    "data:extract:va": "yarn tsn bin/voteamerica.com/extractVoteAmericaData.bin.ts dotenv_config_path=config.sh",
    "data:clean:vg": "yarn tsn bin/vote.gov/cleanVoteGovData.bin.ts dotenv_config_path=config.sh",
    "data:clean:va": "yarn tsn bin/voteamerica.com/cleanVoteAmericaData.bin.ts dotenv_config_path=config.sh",
    "data:parse:vg": "yarn tsn bin/vote.gov/parseVoteGovDeadlines.bin.ts dotenv_config_path=config.sh",
    "data:parse:va": "yarn tsn bin/voteamerica.com/parseVoteAmericaDeadlines.bin.ts dotenv_config_path=config.sh",
    "data:merge": "yarn tsn bin/mergeData.bin.ts dotenv_config_path=config.sh",
    "metrics": "cat metrics.json",
    "metrics:update": "yarn tsn bin/updateMetrics.ts dotenv_config_path=config.sh",
    "format": "yarn prettier:defaults --write",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:prettier": "yarn prettier:defaults --check --debug-check",
    "prettier": "prettier",
    "prettier:defaults": "prettier bin src web",
    "test": "yarn lint && yarn test:jest",
    "test:jest": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "trash": "trash",
    "tsn": "ts-node -r dotenv/config"
  },
  "engines": {
    "node": ">= 12.18"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.13",
    "@types/jsdom": "^16.2.4",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "dayjs": "^1.8.35",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.1",
    "jest": "^26.4.2",
    "jsdom": "^16.4.0",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.2.1",
    "svelte": "^3.25.1",
    "trash-cli": "^3.1.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
